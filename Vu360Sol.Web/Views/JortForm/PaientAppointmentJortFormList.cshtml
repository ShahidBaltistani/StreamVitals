@model IEnumerable<Vu360Sol.ViewModel.JortForm.PatientAppointmentJortFormViewModel>

@{
    ViewBag.Title = "PaientAppointmentJortFormList";
    Layout = "~/Views/Shared/_Theme2Layout.cshtml";
}





<section id="content">
    <div class="content-wrap">
        <div class="container clearfix" style="max-width: 1450px !important">
            <div class="">
                <h1 class="text-center">Paient Appointment Jort Form</h1>
            </div>
            <div class="row gutter-40 col-mb-80">
                <div class="postcontent col-lg-10 offset-1">

                    
                    <div class="row" style="max-width:100% !important">
                        <div class="col-lg-12">
                            <div class="table-responsive">
                                <table class="table" id="content">
                                    <thead>
                                        <tr>
                                            <th>PracticeId</th>
                                            <th>ClinicId</th>
                                            <th>DoctorId</th>
                                            <th>PatientId</th>
                                            <th>First Name</th>
                                            <th>Last Name</th>
                                            <th>Email</th>
                                            <th>Phone</th>
                                            <th>Residential Address_Line1</th>
                                            <th>Residential Address_Line2</th>
                                            <th>Residential City</th>
                                            <th>Residential State</th>
                                            <th>Residential PostalCode</th>
                                            <th>Residential Country</th>
                                            <th>Consent Terms</th>
                                            <th>SearchCountry</th>
                                            <th>ResidentialAddressSearch</th>
                                            <th>Event_Id</th>
                                            <th>CreatedOn</th>

                                        </tr>
                                    </thead>
                                    <tbody id="myTable">
                                        @if (Model.Count() == 0)
                                        {
                                            <tr>
                                                <td colspan="5" style="text-align:center;font-size:20px;font-weight:bold;">
                                                    No data...
                                                    @*<a href="/Doctor/CreateDoctor" style="text-decoration:none">
                                            <small>Create</small>
                                        </a>*@
                                                </td>
                                            </tr>
                                        }
                                        else
                                        {
                                            foreach (var item in Model)
                                            {
                                                <tr id="contentrow">
                                                    <td id="PracticeId">@item.PracticeId</td>
                                                    <td id="ClinicId">@item.ClinicId</td>
                                                    <td id="DoctorId">@item.DoctorId</td>
                                                    <td id="PatientId">@item.PatientId</td>
                                                    <td id="FirstName">@item.FirstName</td>
                                                    <td id="LastName">@item.LastName</td>
                                                    <td id="Email">@item.Email</td>
                                                    <td id="Phone">@item.MobilePhone</td>
                                                    <td id="Phone">@item.ResidentialAddress_Line1</td>
                                                    <td id="Phone">@item.ResidentialAddress_Line2</td>
                                                    <td>@item.ResidentialCity</td>
                                                    <td id="Location">@item.ResidentialState</td>
                                                    <td>@item.ResidentialPostalCode</td>
                                                    <td>@item.ResidentialCountry</td>
                                                    <td>@item.ConsentTerms</td>
                                                    <td>@item.SearchCountry</td>
                                                    <td>@item.ResidentialAddressSearch</td>
                                                    <td>@item.Event_Id</td>
                                                    <td>@item.CreatedOn</td>

                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2"></div>
            </div>
        </div>
    </div>
</section>

